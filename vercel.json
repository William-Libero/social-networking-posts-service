{
  "version": 2,
  "builds": [
    {
      "src": "dist/listener.js",
      "use": "@vercel/node",
      "buildCommand": "nest build --config listener.json"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/listener.js",
      "buildCommand": "nest build --config listener.json"
    }
  ]
}
